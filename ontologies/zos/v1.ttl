vibe:DataLinkLayer a vibe:OSILayer ; rdfs:label "Data Link Layer" .
vibe:NetworkLayer a vibe:OSILayer ; rdfs:label "Network Layer" .
vibe:TransportLayer a vibe:OSILayer ; rdfs:label "Transport Layer" .
vibe:SessionLayer a vibe:OSILayer ; rdfs:label "Session Layer" .
vibe:PresentationLayer a vibe:OSILayer ; rdfs:label "Presentation Layer" .
vibe:ApplicationLayer a vibe:OSILayer ; rdfs:label "Application Layer" .

vibe:VibeStack a vibe:Structure ;
    vibe:hasLayers ( vibe:Layer1 vibe:Layer2 vibe:Layer3 vibe:Layer4 vibe:Layer5 vibe:Layer6 vibe:Layer7 vibe:Layer8 ) .


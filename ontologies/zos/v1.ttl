# Updated Vibe Layer Instances
vibe:Layer1 a vibe:Layer ;
    vibe:hasNumericalAddress 1 ;
    vibe:hasOSILayer vibe:PhysicalLayer ;
    vibe:hasPrimeFactor 2 ; # Example: Physical layer often deals with binary (powers of 2)
    vibe:isPowerOfTwo "true"^^xsd:boolean ;
    vibe:emoji "‚ûï" ;
    vibe:semanticName "BasicBlock", "Number", "Mind", "Loop", "Sum", "Recursion", "Package", "Gene", "Vibe", "Fiber", "Inference", "Insight", "EmojiCode" .

vibe:Layer2 a vibe:Layer ;
    vibe:hasNumericalAddress 2 ;
    vibe:hasOSILayer vibe:DataLinkLayer ;
    vibe:hasPrimeFactor 3 ; # Example: Data Link layer for framing/grouping (threes)
    vibe:isPowerOfTwo "false"^^xsd:boolean ;

@prefix em: <https://rdf.solfunmeme.com/spec/2025/07/17/emoji.ttl#emoji> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# New Vibe Layer Definitions
vibe:OSILayer a owl:Class ;
    rdfs:label "OSI Layer" ;
    rdfs:comment "Represents a layer in the OSI model, used to categorize the functional 'vibe' of a component." .

vibe:Layer a owl:Class ;
    rdfs:subClassOf rdfs:Resource ;
    rdfs:label "Vibe Layer" ;
    rdfs:comment "A conceptual layer representing a functional or semantic 'vibe' within the Code-Math Manifold." ;
    vibe:hasOSILayer rdfs:Resource ; # Link to OSI Layer
    vibe:hasPrimeFactor rdfs:Literal ; # Prime factor associated with the layer
    vibe:isPowerOfTwo rdfs:Literal ; # Boolean indicating if the layer number is a power of two
    vibe:hasNumericalAddress rdfs:Literal . # Unique numerical ID for the layer

# New Project Vibe Definition
vibe:ProjectVibe a owl:Class ;
    rdfs:label "Project Vibe" ;
    rdfs:comment "Represents the overall semantic resonance or 'meme' of a software project." ;
    em:hasEmojiRepresentation rdfs:Literal ;
    vibe:hasSemanticName rdfs:Literal ;
    vibe:hasCrateBloat xsd:decimal ;
    vibe:hasLinesOfCode xsd:integer ;
    vibe:hasFileTreeStructure xsd:string ;
    vibe:hasTestCoverage xsd:decimal ;
    vibe:hasTestCoverageReport xsd:anyURI .


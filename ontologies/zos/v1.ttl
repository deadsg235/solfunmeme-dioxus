    vibe:emoji "Ô∏èÔ∏è" ;
    vibe:semanticName "Seed", "Map", "Mask", "Dial", "Art", "Juice", "Spiral", "Box", "Target", "Lens", "Ice", "Wave", "Curve", "Horn" .

vibe:Layer4 a vibe:Layer ;

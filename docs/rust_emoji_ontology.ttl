@prefix rust: <http://example.org/rust_code#> .
@prefix em: <http://example.org/emoji#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Ontology for Rust Code to Emoji Mappings

# Rust Code Concepts
rust:RustCodeElement a rdfs:Class ;
    rdfs:label "Rust Code Element" ;
    rdfs:comment "A general concept for any identifiable element within Rust source code." .

rust:Function a rust:RustCodeElement ;
    rdfs:label "Rust Function" ;
    rdfs:comment "A Rust function, representing a block of executable code." .

rust:Struct a rust:RustCodeElement ;
    rdfs:label "Rust Struct" ;
    rdfs:comment "A Rust struct, representing a custom data structure." .

rust:Method a rust:RustCodeElement ;
    rdfs:label "Rust Method" ;
    rdfs:comment "A method associated with a Rust struct or enum." .

rust:Variable a rust:RustCodeElement ;
    rdfs:label "Rust Variable" ;
    rdfs:comment "A variable declaration in Rust." .

rust:Loop a rust:RustCodeElement ;
    rdfs:label "Rust Loop" ;
    rdfs:comment "A control flow construct for iteration." .

rust:Conditional a rust:RustCodeElement ;
    rdfs:label "Rust Conditional" ;
    rdfs:comment "A control flow construct for conditional execution." .

# Emoji Mapping Property
em:hasEmojiRepresentation a rdfs:Property ;
    rdfs:label "has emoji representation" ;
    rdfs:domain rust:RustCodeElement ;
    rdfs:range rdfs:Literal ;
    rdfs:comment "The emoji sequence representing a Rust code element's semantic meaning or function." .

